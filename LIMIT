/*
CREATED BY: MEAGAN-FAYE ALLEN
CREATED ON: 05/23/2023
DESCRIPTION: Selects the first 10 records from the customers table, ordered by  ordered by first name
	ascending, then last name descending
*/
SELECT
	LastName 'Last Name',
	FirstName 'First Name',
	Email
FROM
	customers
ORDER BY
	FirstName ASC,
	LastName DESC
LIMIT 10
