/* 
CREATED BY: MEAGAN-FAYE ALLEN
CREATED ON: 05/24/2023
DESCRIPTION: Pull Invoices for  the customer who's total invoices totaling 13.86, 18.86, and 21.86 from the invoices table
*/
SELECT
	InvoiceDate,
	BillingAddress,
	BillingCity,
	total
FROM
	invoices
WHERE
	total IN (13.86, 18.86, 21.86)
ORDER BY
	InvoiceDate
