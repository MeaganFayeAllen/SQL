/* 
CREATED BY: MEAGAN-FAYE ALLEN
CREATED ON:05/22/2023
DESCRIPTION: This script finds all invoices that exceed the average invoice amount generated in 2010
*/
SELECT
InvoiceDate,
total
FROM
invoices
WHERE
total >
(SELECT
AVG(TOTAL) AS '2010 Average Invoice'
FROM
invoices
WHERE
	InvoiceDate BETWEEN '2010-01-01' AND '2010-12-31')
ORDER BY
	Total DESC
